StatusText.TextXAlignment = Enum.TextXAlignment.Left
StatusText.TextColor3 = Color3.fromRGB(180, 180, 180)
StatusText.Text = ""
StatusText.Parent = InfoPanel

local function getHumanoid(plr: Player)
	local char = plr.Character
	if not char then return nil end
	return char:FindFirstChildOfClass("Humanoid")
end

local function setSpectate(plr: Player?)
	if plr and plr.Character then
		local hum = getHumanoid(plr)
		if hum then
			Camera.CameraSubject = hum
			return
		end
	end
	local myHum = getHumanoid(LocalPlayer)
	if myHum then
		Camera.CameraSubject = myHum
	end
end

local function teleportTo(plr: Player?)
	if not plr or not plr.Character then return end
	if not LocalPlayer.Character then return end
	LocalPlayer.Character:PivotTo(plr.Character:GetPivot())
end

local function smallAction(text, y, cb)
	local b = Instance.new("TextButton")
	b.Position = UDim2.fromOffset(14, y)
	b.Size = UDim2.fromOffset(184, 34)
	b.BackgroundColor3 = Color3.fromRGB(28, 28, 34)
	b.TextColor3 = Color3.fromRGB(240, 240, 240)
	b.Font = Enum.Font.GothamSemibold
	b.TextSize = 13
	b.Text = text
	b.Parent = InfoPanel
	Instance.new("UICorner", b).CornerRadius = UDim.new(0, 12)
	b.MouseButton1Click:Connect(cb)
	return b
end

smallAction("Whitelist", 112, function()
	if not SelectedPlayer then return end
	Whitelist[SelectedPlayer.UserId] = true
	StatusText.Text = "Whitelisted: YES"
end)

smallAction("Unwhitelist", 154, function()
	if not SelectedPlayer then return end
	Whitelist[SelectedPlayer.UserId] = nil
	StatusText.Text = "Whitelisted: NO"
end)

smallAction("Spectate", 196, function()
	if not SelectedPlayer then return end
	setSpectate(SelectedPlayer)
end)

smallAction("Unspectate", 238, function()
	setSpectate(nil)
end)

smallAction("Teleport", 280, function()
	if not SelectedPlayer then return end
	teleportTo(SelectedPlayer)
end)

local function updateSelected(plr: Player?)
	SelectedPlayer = plr
	if not plr then
		Avatar.Image = ""
		InfoText.Text = "Select a player"
		StatusText.Text = ""
		return
	end
	local thumb = Players:GetUserThumbnailAsync(plr.UserId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size180x180)
	Avatar.Image = thumb
	InfoText.Text = ("Display: %s\nUsername: %s"):format(plr.DisplayName, plr.Name)
	StatusText.Text = "Whitelisted: " .. (Whitelist[plr.UserId] and "YES" or "NO")
end

local buttons = {}

local function rebuild()
	for _, c in ipairs(List:GetChildren()) do
		if c:IsA("TextButton") then c:Destroy() end
	end
	table.clear(buttons)

	for _, plr in ipairs(Players:GetPlayers()) do
		local btn = Instance.new("TextButton")
		btn.Size = UDim2.new(1, 0, 0, 42)
		btn.BackgroundColor3 = Color3.fromRGB(28, 28, 34)
		btn.TextColor3 = Color3.fromRGB(240, 240, 240)
		btn.Font = Enum.Font.GothamSemibold
		btn.TextSize = 14
		btn.Text = plr.DisplayName
		btn.Parent = List
		Instance.new("UICorner", btn).CornerRadius = UDim.new(0, 12)

		btn.MouseButton1Click:Connect(function()
			for _, b in pairs(buttons) do
				b.BackgroundColor3 = Color3.fromRGB(28, 28, 34)
			end
			btn.BackgroundColor3 = Color3.fromRGB(42, 42, 52)
			updateSelected(plr)
		end)

		buttons[plr.UserId] = btn
	end

	task.defer(function()
		List.CanvasSize = UDim2.new(0, 0, 0, ListLayout.AbsoluteContentSize.Y + 12)
	end)
end

rebuild()
Players.PlayerAdded:Connect(rebuild)
Players.PlayerRemoving:Connect(function(plr)
	if SelectedPlayer == plr then updateSelected(nil) end
	rebuild()
end)

UIS.InputBegan:Connect(function(input, gp)
	if input.KeyCode == Enum.KeyCode.Insert or input.KeyCode == Enum.KeyCode.F4 then
		IsaacTabsUI.Enabled = not IsaacTabsUI.Enabled
	end
end)

print("Fully loaded. lwserboy on dc for more cheats")
