local function makePage()
	local f = Instance.new("Frame")
	f.Size = UDim2.fromScale(1, 1)
	f.BackgroundTransparency = 1
	f.Parent = Content
	return f
end

local PageAimbot = makePage()
local PagePlayers = makePage()
PagePlayers.Visible = false

local function setTab(which)
	PageAimbot.Visible = (which == "Aimbot")
	PagePlayers.Visible = (which == "Players")
	TabAimbotBtn.BackgroundColor3 = (which == "Aimbot") and Color3.fromRGB(42,42,52) or Color3.fromRGB(28,28,34)
	TabPlayersBtn.BackgroundColor3 = (which == "Players") and Color3.fromRGB(42,42,52) or Color3.fromRGB(28,28,34)
end

TabAimbotBtn.MouseButton1Click:Connect(function() setTab("Aimbot") end)
TabPlayersBtn.MouseButton1Click:Connect(function() setTab("Players") end)
setTab("Aimbot")

