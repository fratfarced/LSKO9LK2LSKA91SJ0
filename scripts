-- MADE BY ISSAC

getgenv().Aimbot = {
    Bind = "C",
    HitPart = "HumanoidRootPart",
    Time = 0.167,
    Prediction = { X = 0.12567, Y = 0.08367 },
    Distance = { Reference = 83.67, MaxMultiplier = 3.67 }
}

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local UIS = game:GetService("UserInputService")
local GuiService = game:GetService("GuiService")

local LocalPlayer = Players.LocalPlayer
local Camera = workspace.CurrentCamera

local IsToggled = false
local StickyLock = false
local AimbotMode = "Toggle" -- Toggle / Hold
local AimbotHeld = false
local Target, Cycle

local function GetTarget()
    local closest, shortest = nil, math.huge
    local m = UIS:GetMouseLocation()
    local inset = GuiService:GetGuiInset()
    local mx, my = m.X, m.Y - inset.Y

    for _, p in ipairs(Players:GetPlayers()) do
        if p ~= LocalPlayer and p.Character then
            local hrp = p.Character:FindFirstChild("HumanoidRootPart")
            local hum = p.Character:FindFirstChildOfClass("Humanoid")
            if hrp and hum and hum.Health > 0 then
                local pos, vis = Camera:WorldToViewportPoint(hrp.Position)
                if vis then
                    local d = (Vector2.new(pos.X,pos.Y) - Vector2.new(mx,my)).Magnitude
                    if d < shortest then shortest, closest = d, p end
                end
            end
        end
    end
    return closest
end

local function StartAimbot()
    if Cycle then Cycle:Disconnect() end
    Target = nil
    Cycle = RunService.RenderStepped:Connect(function()
        if not StickyLock or not Target then Target = GetTarget() end
        if not Target or not Target.Character then return end

        local hrp = Target.Character:FindFirstChild("HumanoidRootPart")
        if not hrp then return end

        local vel = hrp.Velocity
        local dist = (Camera.CFrame.Position - hrp.Position).Magnitude
        local mult = math.clamp(dist / getgenv().Aimbot.Distance.Reference, 1, getgenv().Aimbot.Distance.MaxMultiplier)

        local pred = hrp.Position + Vector3.new(
            vel.X * getgenv().Aimbot.Prediction.X * mult,
            vel.Y * getgenv().Aimbot.Prediction.Y,
            vel.Z * getgenv().Aimbot.Prediction.X * mult
        )

        Camera.CFrame = CFrame.lookAt(Camera.CFrame.Position, pred)
    end)
end

local function MatchesBind(input)
    local b = getgenv().Aimbot.Bind
    if input.UserInputType == Enum.UserInputType.Keyboard then
        return input.KeyCode.Name == b
    end
    return (b == "MouseButton4" and input.UserInputType == Enum.UserInputType.MouseButton4)
        or (b == "MouseButton5" and input.UserInputType == Enum.UserInputType.MouseButton5)
end

local UI = Instance.new("ScreenGui", LocalPlayer.PlayerGui)
UI.ResetOnSpawn = false

local Main = Instance.new("Frame", UI)
Main.Size = UDim2.fromOffset(360, 360)
Main.Position = UDim2.fromScale(0.5, 0.5)
Main.AnchorPoint = Vector2.new(0.5,0.5)
Main.BackgroundColor3 = Color3.fromRGB(18,18,18)
Instance.new("UICorner", Main).CornerRadius = UDim.new(0,12)

local Top = Instance.new("Frame", Main)
Top.Size = UDim2.new(1,0,0,36)
Top.BackgroundColor3 = Color3.fromRGB(25,25,25)
Instance.new("UICorner", Top).CornerRadius = UDim.new(0,12)

local Title = Instance.new("TextLabel", Top)
Title.Size = UDim2.fromScale(1,1)
Title.BackgroundTransparency = 1
Title.Text = "Aimbot Settings"
Title.TextColor3 = Color3.new(1,1,1)
Title.Font = Enum.Font.GothamBold
Title.TextSize = 16

local dragging, dragStart, startPos
Top.InputBegan:Connect(function(i)
    if i.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = true
        dragStart = i.Position
        startPos = Main.Position
    end
end)
UIS.InputEnded:Connect(function(i)
    if i.UserInputType == Enum.UserInputType.MouseButton1 then dragging = false end
end)
UIS.InputChanged:Connect(function(i)
    if dragging and i.UserInputType == Enum.UserInputType.MouseMovement then
        local d = i.Position - dragStart
        Main.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset+d.X, startPos.Y.Scale, startPos.Y.Offset+d.Y)
    end
end)

local function slider(text,y,min,max,get,set)
    local lbl = Instance.new("TextLabel", Main)
    lbl.Position = UDim2.fromOffset(16,y)
    lbl.Size = UDim2.fromOffset(330,20)
    lbl.BackgroundTransparency = 1
    lbl.TextColor3 = Color3.new(1,1,1)
    lbl.Font = Enum.Font.Gotham
    lbl.TextSize = 14
    lbl.TextXAlignment = "Left"

    local bar = Instance.new("Frame", Main)
    bar.Position = UDim2.fromOffset(16,y+22)
    bar.Size = UDim2.fromOffset(330,6)
    bar.BackgroundColor3 = Color3.fromRGB(60,60,60)
    Instance.new("UICorner", bar).CornerRadius = UDim.new(1,0)

    local knob = Instance.new("Frame", bar)
    knob.Size = UDim2.fromOffset(14,14)
    knob.AnchorPoint = Vector2.new(0.5,0.5)
    knob.BackgroundColor3 = Color3.fromRGB(220,220,220)
    Instance.new("UICorner", knob).CornerRadius = UDim.new(1,0)

    local drag=false
    local function update()
        local v=get()
        knob.Position = UDim2.fromScale((v-min)/(max-min),0.5)
        lbl.Text = string.format("%s: %.3f",text,v)
    end
    update()

    knob.InputBegan:Connect(function(i) if i.UserInputType==Enum.UserInputType.MouseButton1 then drag=true end end)
    UIS.InputEnded:Connect(function(i) if i.UserInputType==Enum.UserInputType.MouseButton1 then drag=false end end)
    UIS.InputChanged:Connect(function(i)
        if drag and i.UserInputType==Enum.UserInputType.MouseMovement then
            local m=UIS:GetMouseLocation()
            local p=math.clamp((m.X-bar.AbsolutePosition.X)/bar.AbsoluteSize.X,0,1)
            set(min+(max-min)*p)
            update()
        end
    end)
end

slider("Prediction X",50,0,0.5,function()return getgenv().Aimbot.Prediction.X end,function(v)getgenv().Aimbot.Prediction.X=v end)
slider("Prediction Y",100,0,0.5,function()return getgenv().Aimbot.Prediction.Y end,function(v)getgenv().Aimbot.Prediction.Y=v end)
slider("Smoothness",150,0,0.3,function()return getgenv().Aimbot.Time end,function(v)getgenv().Aimbot.Time=v end)

local StickyBtn = Instance.new("TextButton", Main)
StickyBtn.Position = UDim2.fromOffset(16,200)
StickyBtn.Size = UDim2.fromOffset(330,30)
StickyBtn.BackgroundColor3 = Color3.fromRGB(40,40,40)
StickyBtn.TextColor3 = Color3.new(1,1,1)
StickyBtn.Font = Enum.Font.Gotham
StickyBtn.TextSize = 14
StickyBtn.Text = "Sticky Lock: OFF"
Instance.new("UICorner", StickyBtn)
StickyBtn.MouseButton1Click:Connect(function()
    StickyLock = not StickyLock
    Target=nil
    StickyBtn.Text="Sticky Lock: "..(StickyLock and "ON" or "OFF")
end)

local ModeBtn = Instance.new("TextButton", Main)
ModeBtn.Position = UDim2.fromOffset(16,240)
ModeBtn.Size = UDim2.fromOffset(330,30)
ModeBtn.BackgroundColor3 = Color3.fromRGB(40,40,40)
ModeBtn.TextColor3 = Color3.new(1,1,1)
ModeBtn.Font = Enum.Font.Gotham
ModeBtn.TextSize = 14
ModeBtn.Text = "Aimbot Mode: TOGGLE"
Instance.new("UICorner", ModeBtn)
ModeBtn.MouseButton1Click:Connect(function()
    AimbotMode = (AimbotMode=="Toggle") and "Hold" or "Toggle"
    ModeBtn.Text="Aimbot Mode: "..string.upper(AimbotMode)
end)

local KeyBox = Instance.new("TextBox", Main)
KeyBox.Position = UDim2.fromOffset(16,280)
KeyBox.Size = UDim2.fromOffset(330,32)
KeyBox.BackgroundColor3 = Color3.fromRGB(35,35,35)
KeyBox.TextColor3 = Color3.new(1,1,1)
KeyBox.Font = Enum.Font.Gotham
KeyBox.TextSize = 14
KeyBox.Text = "Aimbot Key: "..getgenv().Aimbot.Bind
KeyBox.ClearTextOnFocus=false
Instance.new("UICorner", KeyBox)

local waiting=false
KeyBox.Focused:Connect(function()
    waiting=true
    KeyBox.Text="Press key or mouse button..."
end)

UIS.InputBegan:Connect(function(input,gp)
    if input.KeyCode == Enum.KeyCode.Insert or input.KeyCode == Enum.KeyCode.F4 then
        UI.Enabled = not UI.Enabled
        return
    end

    if waiting then
        if input.UserInputType==Enum.UserInputType.Keyboard then
            getgenv().Aimbot.Bind=input.KeyCode.Name
        elseif input.UserInputType==Enum.UserInputType.MouseButton4 then
            getgenv().Aimbot.Bind="MouseButton4"
        elseif input.UserInputType==Enum.UserInputType.MouseButton5 then
            getgenv().Aimbot.Bind="MouseButton5"
        else return end
        KeyBox.Text="Aimbot Key: "..getgenv().Aimbot.Bind
        waiting=false
        KeyBox:ReleaseFocus()
        return
    end

    if gp then return end
    if MatchesBind(input) then
        if AimbotMode=="Toggle" then
            if not IsToggled then StartAimbot() else if Cycle then Cycle:Disconnect() end Target=nil end
            IsToggled=not IsToggled
        elseif AimbotMode=="Hold" and not AimbotHeld then
            StartAimbot()
            AimbotHeld=true
        end
    end
end)

UIS.InputEnded:Connect(function(input)
    if AimbotMode=="Hold" and MatchesBind(input) then
        if Cycle then Cycle:Disconnect() end
        Target=nil
        AimbotHeld=false
    end
end)

print("lwserboy on discord for more. Insrt/F4 for UI toggle")

