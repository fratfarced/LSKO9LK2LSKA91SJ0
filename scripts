	t.BackgroundTransparency = 1
	t.Position = UDim2.fromOffset(18, y)
	t.Size = UDim2.new(1, -36, 0, 24)
	t.Font = Enum.Font.GothamBold
	t.TextSize = 18
	t.TextXAlignment = Enum.TextXAlignment.Left
	t.TextColor3 = Color3.fromRGB(240, 240, 240)
	t.Text = text
	t.Parent = parent
	return t
end

